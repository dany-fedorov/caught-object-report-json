{
  "name": "caught-object-report-json",
  "version": "0.0.9",
  "description": "Make JS exceptions suitable for structured logging.",
  "main": "./index.js",
  "scripts": {
    "test": "NODE_OPTIONS='--stack-trace-limit=1000' ./node_modules/.bin/jest",
    "build": "tsc -p ./tsconfig.build.json # && tsconfig-replace-paths --project tsconfig.paths.json -s ./src",
    "prepare-me": "rm -rf ./dist && npm run build",
    "prepublish-me": "npm run prepare-me && rm -fr ./npm-module && mv dist ./npm-module && cp package.json ./npm-module && cp README.md ./npm-module",
    "publish-me": "npm run prepublish-me && cd npm-module && npm publish --access public --registry https://registry.npmjs.org/",
    "postpublish-me": "rm -fr npm-module",
    "build-watch": "chokidar './src/**/*.ts' -c 'npm run build' -i './node_modules/**/*' --initial",
    "tsfile": "ts-node -r tsconfig-paths/register -T -s",
    "test-ci": "npm run test -- --ci --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dany-fedorov/caught-object-report-json.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "exception",
    "json logs",
    "structured logging"
  ],
  "author": "Dany Fedorov",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@tsconfig/node16-strictest": "^1.0.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.7.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "ajv": "^8.11.2",
    "axios": "^1.2.1",
    "chokidar": "^3.5.3",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "^28.1.2",
    "prettier": "2.8.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typedoc": "^0.23.23",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "safe-stable-stringify": "2.4.1"
  }
}
