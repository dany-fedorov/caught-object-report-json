{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Caught Object Report JSON",
  "description": "A JSON produced from caught object by JavaScript library. Default implementation is https://www.npmjs.com/package/caught-object-report-json.",
  "type": "object",
  "required": [
    "instanceof_error",
    "typeof",
    "as_string",
    "as_json",
    "_m"
  ],
  "properties": {
    "instanceof_error": {
      "type": "boolean"
    },
    "typeof": {
      "enum": [
        "undefined",
        "object",
        "boolean",
        "number",
        "bigint",
        "string",
        "symbol",
        "function"
      ]
    },
    "constructor_name": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ]
    },
    "message": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ]
    },
    "as_string": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ]
    },
    "as_json": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "number"
        },
        {
          "type": "string"
        },
        {
          "type": "boolean"
        },
        {
          "type": "array"
        },
        {
          "type": "object"
        }
      ]
    },
    "stack": {
      "oneOf": [
        {
          "type": "null"
        },
        {
          "type": "string"
        }
      ]
    },
    "_m": {
      "type": "array",
      "items": false,
      "minItems": 3,
      "maxItems": 3,
      "prefixItems": [
        {
          "enum": [
            "v0.4"
          ]
        },
        {
          "enum": [
            "String"
          ]
        },
        {
          "enum": [
            "safe-stable-stringify@2.4.1"
          ]
        }
      ]
    }
  }
}
